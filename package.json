{
  "name": "advance-client",
  "version": "0.0.2",
  "description": "A toolset for building service oriented JavaScript applications with React, Redux, and AWS.",
  "main": "index.js",
  "scripts": {
    "build": "babel-node --presets es2015 $(npm bin)/webpack --config scripts/build/prod.config.js",
    "dev": "npm run fetch-schema && babel-node --presets es2015 $(npm bin)/webpack-dev-server --config scripts/build/dev.config.js",
    "prod": "babel-node --presets es2015 $(npm bin)/webpack --config scripts/build/prod.config.js --watch",
    "test:unit": "babel-node --presets es2015 scripts/test/unit",
    "test:integration": "babel-node --presets es2015 scripts/test/integration",
    "test:acceptance": "babel-node --presets es2015 scripts/test/acceptance",
    "deploy": "babel-node --presets es2015 scripts/deploy",
    "clean": "rimraf ./dist",
    "fetch-schema": "apollo-codegen introspect-schema https://api.graph.cool/simple/v1/ciyp5mu7q47df0132db4vojmp --output schema.json"
  },
  "author": "Tyler Martinez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:slightlytyler/advance-client.git"
  },
  "dependencies": {
    "apollo-client": "^1.0.0-rc.2",
    "auth0-lock-passwordless": "^2.2.3",
    "connected-react-router": "^4.0.0-beta.3",
    "element-resize-detector": "^1.1.10",
    "graphql-tag": "^1.3.1",
    "immutability-helper": "^2.1.1",
    "invariant": "^2.2.2",
    "jss": "^6.2.0",
    "jss-preset-default": "^1.3.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "moment": "^2.17.1",
    "qs": "^6.3.0",
    "react": "^15.4.2",
    "react-apollo": "^1.0.0-rc.2",
    "react-async-component": "^0.2.2",
    "react-dom": "^15.4.2",
    "react-formal": "^0.24.3",
    "react-jss": "^5.3.0",
    "react-layout-components": "^3.0.4",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "reselect": "^2.5.4",
    "shortid": "^2.2.6",
    "warning": "^3.0.0",
    "whatwg-fetch": "^2.0.2",
    "yup": "^0.21.3"
  },
  "devDependencies": {
    "apollo-codegen": "^0.10.9",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-class-property": "^1.0.3",
    "eslint-plugin-graphql": "^0.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "graphql": "^0.9.1",
    "html-webpack-plugin": "^2.28.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "serialize-javascript": "^1.3.0",
    "surge": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^4.0.0"
  }
}
