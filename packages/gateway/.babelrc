{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "node": "current",
      },
    }],
    "stage-0",
  ],
  "env": {
    "development": {
      "plugins": [
        ["flow-runtime", {
          "annotate": true,
          "assert": true,
        }],
        "transform-flow-strip-types",
      ],
    },
    "production": {
      "plugins": ["transform-flow-strip-types"],
    },
  },
}
